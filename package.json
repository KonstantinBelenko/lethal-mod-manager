{
  "name": "lorca-ts-react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "lucide-react": "^0.316.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.8",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "jest": "^27.5.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "concurrently \"npm run react:start\" \"npm run go:start\"",
    "build": "npm run react:build && npm run go:generate && npm run go:build",
    "test": "npm run go:test && npm run react:test",
    "go:start": "go build -tags debug -o bin/app.exe && .\\bin\\app.exe",
    "go:generate": "go generate",
    "go:build": "go build -o bin/app.exe",
    "go:test": "go test ./...",
    "react:start": "cross-env BROWSER=none react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
