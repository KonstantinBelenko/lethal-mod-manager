{
  "name": "lorca-ts-react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "cross-env": "^7.0.3",
    "lucide-react": "^0.316.0",
    "next-themes": "^0.2.1",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.8.9"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.8",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^27.5.1",
    "react-scripts": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "concurrently \"npm run react:start\" \"npm run go:start\"",
    "build": "npm run react:build && npm run go:generate && npm run go:build",
    "test": "npm run go:test && npm run react:test",
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "go:start": "go build -tags debug -o bin/app.exe && .\\bin\\app.exe",
    "go:generate": "go generate",
    "go:build": "go build -o bin/app.exe",
    "go:test": "go test ./...",
    "react:start": "cross-env BROWSER=none react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
